{
  "name": "repo-template",
  "version": "1.0.0",
  "private": true,
  "description": "A template for bootstrapping a repo",
  "homepage": "https://github.com/DanMad/repo-template#readme",
  "bugs": "https://github.com/DanMad/repo-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/repo-template.git"
  },
  "license": "MIT",
  "author": "DanMad",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn install && husky install && chmod +x .husky/*",
    "build": "rm -rf dist && webpack",
    "format": "sort-package-json && prettier --write \"**/*\"",
    "start": "webpack serve --env development"
  },
  "dependencies": {
    "on-the-case": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "sort-package-json": "^1.52.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
