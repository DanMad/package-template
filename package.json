{
  "name": "repo-template",
  "version": "1.0.0",
  "private": true,
  "description": "A template for bootstrapping a repo",
  "homepage": "https://github.com/DanMad/repo-template#readme",
  "bugs": "https://github.com/DanMad/repo-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/repo-template.git"
  },
  "license": "MIT",
  "author": "DanMad",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn install && husky install && chmod +x .husky/*",
    "build": "rm -rf dist && webpack",
    "format": "sort-package-json && prettier --write \"**/*\"",
    "start": "webpack serve --env development"
  },
  "dependencies": {
    "on-the-case": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "sort-package-json": "^1.54.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
