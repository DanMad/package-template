{
  "name": "repo-template",
  "version": "1.0.0",
  "private": true,
  "description": "A template for bootstrapping a repo",
  "homepage": "https://github.com/DanMad/repo-template#readme",
  "bugs": "https://github.com/DanMad/repo-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/repo-template.git"
  },
  "license": "MIT",
  "author": "DanMad",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn install && husky install && chmod +x .husky/*",
    "build": "rm -rf dist && webpack",
    "format": "sort-package-json && prettier --write \"**/*\"",
    "start": "webpack serve --env development"
  },
  "dependencies": {
    "on-the-case": "^2.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.15.3",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.0",
    "sass": "^1.39.2",
    "sass-loader": "^12.1.0",
    "sort-package-json": "^1.51.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0"
  }
}
