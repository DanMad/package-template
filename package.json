{
  "name": "@danmad/package-name",
  "version": "1.0.0",
  "description": "A handy package template",
  "homepage": "https://github.com/danmad/package-name#readme",
  "bugs": "https://github.com/danmad/package-name/issues",
  "repository": "https://github.com/danmad/package-name.git",
  "license": "UNLICENSED",
  "author": "Daniel Maddison <hooky.mounted_0n@icloud.com>",
  "scripts": {
    "bootstrap": "yarn install --frozen-lockfile && husky install && chmod +x .husky/*",
    "build": "rm -rf dist/* && webpack",
    "nuke": "rm -rf node_modules && rm -f yarn.lock",
    "ship": "cd dist && yarn publish --access public",
    "tidy": "sort-package-json && prettier --write '**/*'"
  },
  "dependencies": {
    "on-the-case": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/register": "^7.17.7",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.1",
    "sort-package-json": "^1.57.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  }
}
